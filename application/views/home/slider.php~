                        <script>
                            function changeBG(interval, frames) {

                                var i = 1;

                                //$("#bgIMG"+i).css("opacity", "100");
                                jQuery(".imgForLoad").append("&lt;img id=\"bgIMG1\" src=\"/sites/all/themes/zizabi_v1/img/homepageBG/1.jpg\" /&gt;");

                                function func() {
                                    //console.log("CHANGING IMAGE");
                                    //document.body.id = "b"+int;
                                    jQuery('.home-page-background').css("background-image", "url(/sites/all/themes/zizabi_v1/img/homepageBG/"+i+".jpg)");
                                    if(i&lt;=0) i = frames;
                                    //$("#bgIMG"+prevI).css("opacity", "0");
                                    //$("#bgIMG"+i).css("opacity", "100");
                                    i++;
                                    if(i &gt; frames) { i = 1; }

                                    if(jQuery(".imgForLoad #bgIMG"+i).length == 0)
                                    {
                                        jQuery(".imgForLoad").append("&lt;img id=\"bgIMG"+i+"\" src=\"/sites/all/themes/zizabi_v1/img/homepageBG/"+i+".jpg\" /&gt;");
                                    }
                                }

                                var swap = window.setInterval(func, interval);
                            }
                            changeBG(8000, 6);
                        </script>  </div>
